<?xml version="1.0" encoding="UTF-8"?>

<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xmlns:context="http://www.springframework.org/schema/context"
       xsi:schemaLocation="http://www.springframework.org/schema/beans 
           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
           http://www.springframework.org/schema/context
           http://www.springframework.org/schema/context/spring-context-3.0.xsd">
           
      <context:annotation-config></context:annotation-config>         
     <bean id="humanHeart" class="spring_autowiered_qualifier.Heart">
     <property name="nameOfAnimal" value="Human"></property>
     <property name="noOfHeart" value="1"></property>  
     </bean>
     <bean id="animalHeart" class="spring_autowiered_qualifier.Heart">
     <property name="nameOfAnimal" value="Octobus"></property>
     <property name="noOfHeart" value="3"></property> 
     </bean>
     
     <bean id="human" class="spring_autowiered_qualifier.Human"></bean>
     
</beans>